{
  "manifest_version": 2,
  "name": "estimate-compression-level",
  "description": "A Chrome browser extension that estimates the HTTP compression level of the page.",
  "version": "0.0.1",
  "icons": {
    "16": "images/pc16.png",
    "48": "images/pc48.png",
    "128": "images/pc128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/pc-off.png",
      "38": "images/pc-off-hidpi.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["webRequest", "webNavigation", "http://*/*", "https://*/*"]
}
